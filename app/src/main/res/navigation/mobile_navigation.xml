<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.askapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.askapp.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <activity
        android:id="@+id/navigation_questions"
        android:name="com.example.askapp.ui.questions.activityQuestion"
        android:label="@string/title_questions"
        tools:layout="@layout/activity_question" />

    <fragment
        android:id="@+id/navigation_register"
        android:name="com.example.askapp.ui.register.RegisterFragment"
        android:label="Register"
        tools:layout="@layout/register_fragment" />

</navigation>